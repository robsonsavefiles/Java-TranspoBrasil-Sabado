<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="brand" name="br.com.invnautico.core.model.INMembership">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property name="nome" type="string" column="nome"/>
    <property name="ativo" type="boolean" column="active"/>
    <property name="site" type="string" column="website"/>
    <property name="dados" type="string" column="member_data"/>
  </class>
</hibernate-mapping>
